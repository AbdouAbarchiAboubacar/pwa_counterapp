{
    "name": "Counter App",
    "short_name": "Counter App",
    "id": ".",
    "start_url": ".",
    "display": "standalone",
    "background_color": "#0175C2",
    "theme_color": "#0175C2",
    "description": "Counter App",
    "orientation": "portrait-primary",
    "prefer_related_applications": false,
    "categories": [
        "news"
    ],
    "launch_handler": {
        "client_mode": [
            "navigate-existing, auto"
        ]
    },
    "screenshots": [
        {
            "src": "icons/screenshot1.png",
            "type": "image/png",
            "sizes": "540x720",
            "form_factor": "narrow"
        }
    ],
    "file_handlers": [
        {
            "action": "/open-pdf",
            "accept": {
                "application/pdf": [
                    ".pdf"
                ]
            },
            "icons": [
                {
                    "src": "icons/pdf-icon.png",
                    "sizes": "256x256",
                    "type": "image/png"
                }
            ],
            "launch_type": "single-client"
        }
    ],
    "display_override": [
        "window-controls-overlay",
        "standalone",
        "browser"
    ],
    "icons": [
        {
            "src": "icons/Icon-192.png",
            "sizes": "192x192",
            "type": "image/png"
        },
        {
            "src": "icons/Icon-512.png",
            "sizes": "512x512",
            "type": "image/png"
        },
        {
            "src": "icons/Icon-maskable-192.png",
            "sizes": "192x192",
            "type": "image/png",
            "purpose": "maskable"
        },
        {
            "src": "icons/Icon-maskable-512.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "maskable"
        }
    ]
}